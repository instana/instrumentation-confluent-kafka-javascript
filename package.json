{
  "name": "@instana/instrumentation-confluent-kafka-javascript",
  "version": "1.0.0",
  "description": "OpenTelemetry instrumentation for `@confluentinc/kafka-javascript` messaging client for Apache Kafka",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/instana/instrumentation-confluent-kafka-javascript.git"
  },
  "scripts": {
    "pretest": "npm run build",
    "test": "nyc mocha --require @opentelemetry/contrib-test-utils 'test/**/*.test.ts'",
    "test-all-versions": "tav",
    "tdd": "npm run test -- --watch-extensions ts --watch",
    "clean": "rimraf lib/* tsconfig.tsbuildinfo",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prewatch": "npm run prebuild",
    "prepublishOnly": "npm run build",
    "build": "tsc -p .",
    "prepare": "npm run build"
  },
  "keywords": [
    "@confluentinc/kafka-javascript",
    "confluent-kafka-javascript",
    "instrumentation",
    "nodejs",
    "opentelemetry",
    "profiling",
    "tracing"
  ],
  "authors": ["kirrg001", "Drazke"],
  "license": "Apache-2.0",
  "engines": {
    "node": "^18.19.0 || >=20.6.0"
  },
  "files": [
    "lib/*.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.3.0"
  },
  "devDependencies": {
    "@confluentinc/kafka-javascript": "^1.3.0",
    "@opentelemetry/api": "^1.3.0",
    "@opentelemetry/contrib-test-utils": "^0.46.0",
    "@opentelemetry/sdk-trace-base": "^2.0.0",
    "@types/mocha": "10.0.10",
    "@types/node": "18.18.14",
    "@types/sinon": "^17.0.0",
    "generate-changelog": "^1.8.0",
    "mocha": "^11.2.2",
    "nyc": "17.1.0",
    "rimraf": "5.0.10",
    "sinon": "15.2.0",
    "test-all-versions": "6.1.0",
    "ts-node": "^10.9.2",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "@opentelemetry/instrumentation": "^0.207.0",
    "@opentelemetry/semantic-conventions": "^1.34.0"
  },
  "homepage": "https://github.com/instana/instrumentation-confluent-kafka-javascript/tree/main/#readme"
}
